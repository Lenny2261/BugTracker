@model IEnumerable<BugTracker.Models.ApplicationUser>

@foreach (var item in Model)
{
    <tr>
        <td>
            @item.firstName
        </td>
        <td>
            @item.lastName
        </td>
        <td class="text-right">
            Assign to Projects: @Html.ActionLink("Edit", "AssignUsers", new { userId = item.Id })
        </td>
    </tr>
    <hr />
}